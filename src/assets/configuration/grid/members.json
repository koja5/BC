{
    "type": "members",
    "new": {
        "open": "modal"
    },
    "search": {
        "display": true
    },
    "columns": [
        {
            "type": "image",
            "field": "image",
            "rootPath": "../../../../assets/img/profile_image/"
        },
        {
            "type": "data",
            "field": "id"
        },
        {
            "type": "data",
            "field": "email"
        },
        {
            "type": "data",
            "field": "firstname"
        },
        {
            "type": "data",
            "field": "lastname"
        },
        {
            "type": "boolean",
            "action": "active",
            "field": "active"
        },
        {
            "type": "button",
            "action": "edit-member",
            "class": "k-preview", 
            "icon": "k-icon k-i-edit",
            "method": "editUser"
        },
        {
            "type": "button",
            "action": "delete",
            "class": "k-preview", 
            "icon": "k-icon k-i-delete",
            "method": "deleteMember",
            "param": "id"
        }
    ],
    "exportPdfName": "Members.pdf",
    "exportExcelName": "Members.xlsx"
}